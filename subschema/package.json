{
  "name": "subschema",
  "version": "4.0.0-b-1",
  "description": "Subschema",
  "main": "lib",
  "scripts": {
    "compile-default-loader": "subschema-autoloader | subschema-babel --out-file  ./lib/DefaultLoader.js && subschema-importer | subschema-babel --out-file ./lib/importer.js",
    "compile-lib": "subschema-babel && yarn run compile-default-loader",
    "compile-server": "subschema-webpack --output-path dist  --output-filename=subschema.server.js --target node",
    "compile-web": "NODE_ENV=production subschema-webpack --use-externals=false --devtool sourcemap -p  --output-path dist  --output-filename subschema.js",
    "compile-web-debug": "subschema-webpack --use-externals=false --output-library-target=umd --devtool inline-source-map --output-path dist --output-filename subschema-debug.js",
    "compile-web-noreact": "subschema-webpack -p  --output-path dist --output-filename subschema-noreact.js",
    "karma": "subschema-karma",
    "test": "subschema-karma",
    "prepublish": "yarn run compile-lib && yarn run compile-web && yarn run compile-web-debug && yarn run compile-web-noreact",
    "clean": "subschema-clean"
  },
  "peerDependencies": {
    "react": "16.x || ^15.5.0",
    "react-dom": "16.x || ^15.5.0"
  },
  "dependencies": {
    "subschema-preset-web-form": "^4.0.0-b-1",
    "subschema-preset-resolvers": "^4.0.0-b-1",
    "subschema-preset-wizard": "^4.0.0-b-1",
    "subschema-preset-list": "^4.0.0-b-1",
    "subschema-context": "^4.0.0-b-1",
    "subschema-plugin-autoloader": "^4.0.0-b-1"
  },
  "devDependencies": {
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "subschema-test-support": "^4.0.0-b-1",
    "subschema-dev-support": "^4.0.0-b-1"
  },
  "mrbuilder": {
    "plugin":"./mrbuilder-plugin",
    "plugins": [
      [
        "mrbuilder-plugin-webpack",
        {
          "libaryTarget": "umd"
        }
      ],
      [
        "mrbuilder-plugin-html",
        false
      ],
      "subschema-plugin-autoloader",
      "subschema"
    ]
  },
  "subschema": {
    "options": {
      "loader": false
    },
    "plugins": [
      [
        "subschema-prop-types",
        "subschema-plugin-autoloader"
      ]
    ],
    "presets": [
      "subschema-preset-list",
      "subschema-preset-wizard",
      "subschema-preset-web-form",
      "subschema-preset-resolvers"
    ]
  },
  "source": "src"
}
