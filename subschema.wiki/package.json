{
  "name": "subschema.wiki",
  "version": "4.0.0-b-1",
  "description": "Subschema Wiki",
  "main": "lib",
  "scripts": {
    "server": "subschema-webpack-dev-server",
    "clean": "rimraf ./lib ./dist",
    "prepublish": "subschema-webpack"
  },
  "devDependencies": {
    "react": "^16.1.0",
    "rimraf": "^2.6.2",
    "subschema-dev-support": "^4.0.0-b-1",
    "subschema-dev-markdown": "^4.0.0-b-1"
  },
  "peerDependencies": {
    "babel-standalone": "*"
  },
  "source": "src",
  "subschema": {
    "options": {
      "loader": "Doc"
    },
    "plugins": [
      "subschema-dev-support",
      [
        "subschema-dev-markdown",
        {
          "loader": false
        }
      ]
    ]
  }
}
