{
  "name": "subschema-demo",
  "version": "4.0.0-b-1.6",
  "dependencies": {
    "subschema": "^4.0.0-b-1.6",
    "subschema-plugin-autoloader": "^4.0.0-b-1.6",
    "subschema-plugin-demo": "^4.0.0-b-1.6",
    "subschema-plugin-project": "^4.0.0-b-1.6",
    "subschema-preset-examples": "^4.0.0-b-1.6",
    "subschema-preset-resolvers": "^4.0.0-b-1.6",
    "subschema-preset-web-form": "^4.0.0-b-1.6",
    "subschema-styles-bootstrap4": "^4.0.0-b-1.6",
    "subschema.wiki": "^4.0.0-b-1.6"
  },
  "devDependencies": {
    "subschema-dev-support": "^4.0.0-b-1.6"
  },
  "scripts": {
    "start": "subschema-webpack-dev-server",
    "demo": "SUBSCHEMA_USE_ANALYTICS='UA-72459938-1' subschema-webpack --demo ../docs/dist/demo",
    "server": "subschema-webpack-dev-server"
  },
  "subschema": {
    "plugins": [
      [
        "subschema-styles-bootstrap",
        false
      ],
      "subschema-styles-bootstrap4",
      "subschema-plugin-project",
      "subschema-plugin-playground",
      "subschema-plugin-demo",
      [
        "subschema.wiki",
        {
          "loader": "Doc"
        }
      ]
    ],
    "presets": [
      "subschema",
      "subschema-preset-examples"
    ]
  },
  "source": "src",
  "main": "lib",
  "mrbuilder": {
    "presets": [
      "subschema-styles-bootstrap4"
    ],
    "plugins": [
      "mrbuilder-plugin-markdown",
      [
        "mrbuilder-plugin-html",
        {
          "exported": true
        }
      ],
      [
        "mrbuilder-plugin-webpack",
        {
          "entry": {
            "index": "./src/index"
          }
        }
      ],
      [
        "mrbuilder-plugin-version",
        {
          "module": "~subschema/package.json"
        }
      ],
      "subschema",
      "subschema-plugin-autoloader",
      "subschema-plugin-project"
    ]
  }
}
