{
  "name": "subschema-styles-bootstrap4",
  "version": "4.1.2",
  "main": "lib",
  "description": "Bootstrap4 CSS support for subschema",
  "scripts": {
    "clean": "subschema-clean",
    "prepare": "NODE_ENV=development subschema-webpack"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "mrbuilder-plugin-sass": "^3.3.1",
    "subschema-transitions": "^4.1.2",
    "subschema-utils": "^4.1.2"
  },
  "devDependencies": {
    "subschema-dev-support": "^4.1.2"
  },
  "source": "src",
  "mrbuilder": {
    "plugins": [
      "mrbuilder-plugin-filetypes",
      "mrbuilder-plugin-fonts",
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": false
        }
      ],
      "mrbuilder-plugin-sass"
    ]
  },
  "license": "MIT"
}