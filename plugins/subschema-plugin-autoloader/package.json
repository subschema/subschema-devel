{
  "name": "subschema-plugin-autoloader",
  "version": "4.0.0-b-1",
  "bin": {
    "subschema-autoloader": "bin/subschema-autoloader.js",
    "subschema-importer": "bin/subschema-importer.js"
  },
  "scripts": {
    "clean": "subschema-clean",
    "prepublish": "./bin/subschema-autoloader.js > lib/index.js && ./bin/subschema-importer.js > lib/importer.js"
  },
  "source": "src",
  "main": "lib",
  "dependencies": {
    "mrbuilder-optionsmanager": "^0.9.6"
  },
  "devDependencies": {
    "subschema-dev-support": "^4.0.0-b-1"
  },
  "subschema": {
    "options": {
      "loader": false
    }
  },
  "mrbuilder": {
    "plugin": "./mrbuilder-plugin.js",
    "plugins": [
      "subschema-plugin-autoloader"
    ]
  }
}
